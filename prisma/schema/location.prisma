model Location {
    id          Int      @id @default(autoincrement())
    block       String
    room        String
    description String   @default("")
    createdAt   DateTime @default(now())
    updatedAt   DateTime @updatedAt
    updatedBy   User?    @relation(fields: [updatedById], references: [id])
    updatedById Int?

    Local      Local[]
    Equipament Equipament[]

    @@unique([block, room])
}
